# system of 10 chains surrounded by solvent
# density = 10 %

tolerance 1.25
filetype pdb
output system_small.pdb

structure oneChain.pdb
    number 30
    resnumber 3
    inside box 20.0 20.0 20.0 180.0 180.0 180.0 
end structure

structure polarSolvent.pdb
    number 41700
    resnumber 3
    inside box 0.0 0.0 0.0 200.0 200.0 200.0
end structure

